---
import site from "../../config/navbar.json";
import SimpleNavbar from "./SimpleNavbar.astro";
import LogoNavbar from "./LogoNavbar.astro";

// Typ navbaru z configu â€“ "simple" alebo "logo"
const NavbarType = site.navbarType || "simple";
const { sticky = false } = Astro.props;
---
{NavbarType === "logo" ? (
  <LogoNavbar sticky={sticky} />
) : (
  <SimpleNavbar sticky={sticky} />
)}
